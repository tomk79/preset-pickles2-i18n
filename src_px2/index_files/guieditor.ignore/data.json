                                    "fields": {
                                        "main": {
                                            "src": "## 次のステップ",
                                            "editor": "markdown"
                                        },
                                        "paragraph": {
                                            "src": "px2-p"
                                        },
                                        "text-align": {
                                            "src": ""
                                        },
